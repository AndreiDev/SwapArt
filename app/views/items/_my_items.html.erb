<div class="gallery_thumbnail">
  <ul class="row">
    <% locals[:my_items_user_wants].each do |item| %>
      <li class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="thumbnail">
          <div class="thumbnail-image-div">
            <img src= <%= S3_PATH + item.image_url %> alt="">
          </div>
        </div>
      </li>
    <% end %>
  </ul>
</div>
