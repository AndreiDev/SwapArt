$('#modal-injection').empty();
<% @from = " מ" %>
<% @swapping_with = "החלפת תמונות עם" %>
$('#UserModalLabel').text("<%= @swapping_with %> <%= @user.first_name %> <%= @from %><%= @user.region.description %>")
<% @user_items.each do |item| %>
$("<%= escape_javascript render(:file => 'items/_item_block', locals: {locals: {item: item, type: :modal}}) %>").appendTo('#modal-injection');
<% end %>

$('#UserModal').modal();
