$('#UserContactModal .first_name').text("<%= @user.first_name %>");
$('#UserContactModal .last_name').text(" <%= @user.last_name %>");
$('#UserContactModal .region').text("<%= @user.region.description %>");
$('#UserContactModal .user_email').text("<%= @user.email %>");
$('#UserContactModal .user_email').attr("href", "mailto:<%= @user.email %>");
$('#UserContactModal .user_phone a').attr("href", "tel:<%= @user.phone %>");
$('#UserContactModal .user_phone a').text("<%= @user.phone %>");
$('#UserModal').modal('hide');
$('#UserContactModal').modal();